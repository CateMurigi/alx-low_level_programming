#include "main.h"
#include <stdio.h>

/**
 * main - entry point
 * times_table - prints the table
 *
 * Return: Always 0
 */

int times_table(void)
{
	int a, b;

	for (a = 0 ; a <= 9 ; a++)
	{
		for (b = 0 ; b <= 9 ; b++)
		{
			printf("%d ", a * b);
		}
		printf("\n");
	}
}

int main(void)
{
	time_table();
	return (0);
}
